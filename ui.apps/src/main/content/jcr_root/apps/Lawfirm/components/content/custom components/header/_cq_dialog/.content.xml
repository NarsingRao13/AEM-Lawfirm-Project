<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <items
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <logo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                        fieldDescription="Can not be empty"
                                        fieldLabel="Logo"
                                        name="./logo"/>
                                    <logo-link-url
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                        fieldDescription="Links the title. Path to a content page, external URL or page anchor."
                                        fieldLabel="Logo LInk URL"
                                        name="./logoLInkURL"
                                        rootPath="/content/Lawfirm/us/en"/>
                                    <navigation-bar
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Navigation">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./homeNavigationList">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <nav-title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Can not be empty"
                                                            fieldLabel="Title"
                                                            name="./navTitle"/>
                                                        <nav-link-url
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                            fieldDescription="Links the title. Path to a content page, external URL or page anchor."
                                                            fieldLabel="Nav LInk URL"
                                                            name="./navLInkURL"
                                                            rootPath="/content/Lawfirm/us/en"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </navigation-bar>
                                </items>
                            </column>
                        </items>
                    </columns>
                </items>
            </items>
        </items>
    </content>
</jcr:root>
